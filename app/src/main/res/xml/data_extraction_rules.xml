<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include database in cloud backups -->
        <include domain="database" />
        <include domain="sharedpref" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Include database in device transfers -->
        <include domain="database" />
        <include domain="sharedpref" />
    </device-transfer>
</data-extraction-rules>